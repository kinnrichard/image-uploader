{
  "name": "fxpro-imagify",
  "version": "1.0.0",
  "description": "FXPRO IMAGIFY - Professional image sharing platform with login functionality",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm ci --only=production",
    "test": "echo \"No tests specified - skipping\" && exit 0",
    "docker:build": "docker build -t image-uploader .",
    "docker:run": "docker run -p 3000:3000 image-uploader",
