    "docker:compose:logs": "docker-compose logs -f",
    "production": "NODE_ENV=production npm start"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
